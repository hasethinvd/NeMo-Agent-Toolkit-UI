{
  "name": "NAT Toolkit-UI",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:http": "NEXT_PUBLIC_API_PROTOCOL=http NEXT_PUBLIC_API_HOST=localhost NEXT_PUBLIC_API_PORT=8080 NEXT_PUBLIC_HTTP_CHAT_COMPLETION_URL=http://localhost:8080/chat/stream NEXT_PUBLIC_WS_CHAT_COMPLETION_URL=ws://localhost:8080/websocket NEXT_PUBLIC_WEB_SOCKET_DEFAULT_ON=true next dev",
    "dev:https": "NODE_TLS_REJECT_UNAUTHORIZED=0 NEXT_PUBLIC_API_PROTOCOL=https NEXT_PUBLIC_API_HOST=0.0.0.0 NEXT_PUBLIC_API_PORT=8080 NEXT_PUBLIC_HTTP_CHAT_COMPLETION_URL=https://0.0.0.0:8080/chat/stream NEXT_PUBLIC_WS_CHAT_COMPLETION_URL=wss://0.0.0.0:8080/websocket NEXT_PUBLIC_WEB_SOCKET_DEFAULT_ON=true next dev",
    "dev:prod-ssl": "HTTPS=true SSL_CRT_FILE=../../certs/prod.crt SSL_KEY_FILE=../../certs/prod.key next dev --experimental-https",
    "dev:port": "NEXT_PUBLIC_API_PORT=${API_PORT:-8080} NEXT_PUBLIC_API_PROTOCOL=${API_PROTOCOL:-https} next dev",
    "dev:flexible": "./scripts/start-ui.sh",
    "build": "next build",
    "start": "next start",
    "start:https": "HTTPS=true SSL_CRT_FILE=../../certs/prod.crt SSL_KEY_FILE=../../certs/prod.key next start --experimental-https",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
